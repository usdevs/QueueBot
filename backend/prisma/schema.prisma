generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Organiser {
  id          Int @id @default(autoincrement())
  telegram_id Int @unique
}

model QueueConfig {
  id                 Int     @id
  positionBeforePing Int
  isOpen             Boolean
}

model Queue {
  telegram_id Int      @unique
  timeCreated DateTime
}
