generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Admin {
  id          Int    @id @default(autoincrement())
  telegram_id BigInt @unique
}

model QueueConfig {
  id                 Int     @id @default(autoincrement())
  positionBeforePing Int
  isOpen             Boolean
}

model Queue {
  telegram_id BigInt   @unique
  timeCreated DateTime
}

model AdminRequester {
  id                Int    @id @default(autoincrement())
  telegram_id       BigInt @unique
  telegram_username String
}
