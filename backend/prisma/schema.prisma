generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Organiser {
  id          Int    @id @default(autoincrement())
  telegram_id BigInt @unique
}

model QueueConfig {
  id                 Int     @id @default(autoincrement())
  positionBeforePing Int
  isOpen             Boolean
}

model Queue {
  telegram_id BigInt   @unique
  timeCreated DateTime
}
